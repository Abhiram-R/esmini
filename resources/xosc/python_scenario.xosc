                    </Actors>
                    <Maneuver name="ManeuverLaneChange">
                        <Event name="lanechangeEvent1" priority="overwrite" maximumExecutionCount="1">
                            <Action name="brake">
                                <PrivateAction>
                                    <LongitudinalAction>
                                        <SpeedAction>
                                            <SpeedActionDynamics dynamicsShape="linear" value="3" dynamicsDimension="time"/>
                                            <SpeedActionTarget>
                                                <AbsoluteTargetSpeed value="0"/>
